import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.err.println("input height and width");
		int i = sc.nextInt();
		int j = sc.nextInt();
		makeCoub(i,j);
	}public static void getWidth(int a) {
		for (int i=0; i<=a;i++) {
			System.out.print("*");
		}
	}public static void makeCoub(int a,int b) {
		for (int i =0;i<=b;i++) {
			getWidth(a);
			System.out.println();
		}
	}

}
